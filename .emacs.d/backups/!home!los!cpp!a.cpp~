#include <string.h>
#include <cstdlib>
#include <stdio.h>
#include <vector>
#include <algorithm>
#include <iostream>
#include <tuple>



using namespace std;


bool my_eq(const vector<int> & data)  {
  vector<int> diffs;
  for ( int i=0; i<data.size(); ++i )
    diffs.push_back( data[i]-i );
  sort(diffs.begin(), diffs.end());
  int p1 = 0;
  int p2 = data.size() - 1;
  while ( p1 < p2 )  {
    int sum = diffs[p1] + diffs[p2];
    if ( 0==sum )
      return true;
    if ( sum < 0 )
      ++p1;
    else
      --p2;
  }
  return false;
}
 

int main( int argc, char *argv[] )  {
  cout << my_eq( vector<int>{1,2,4,8,16} ) << endl;
  cout << my_eq( vector<int>{1,3,5,4,6,7,7,7,7} ) << endl;
  cout << my_eq( vector<int>{211,133,255,144,266,177,111,233,155,244,166,277} ) << endl;
 
}
